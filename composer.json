{
    "name": "simplesoftwareio/simple-qrcode",
    "description": "Simple QrCode is a QR code generator made for Laravel.",
    "keywords": ["qrcode","generator","qrcode generator", "merge", "icon", "frame", "laravel"],
    "license" : "MIT",
    "authors": [
        {
            "name": "wenxin",
            "email": "gwenxin0909@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
      	"ext-gd": "^7.1",
        "bacon/bacon-qr-code": "^2.0",
        "illuminate/support": "^5.8",
	"intervention/image": "^2.4"
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "Wenxin\\Customqrcode\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wenxin\\Customqrcode\\QrCodeServiceProvider"
            ],
            "aliases": {
                "QrCodeGen": "Wenxin\\Customqrcode\\Facades\\QrCodeGen"
            }
        }
    }
}
