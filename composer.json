{
    "name": "wenxin/customqrcode",
    "description": "Simple QrCode is a QR code generator made for Laravel.",
    "keywords": ["qrcode", "customize", "generator", "icon"],
    "license" : "MIT",
    "authors": [
        {
            "name": "wenxin",
            "email": "gwenxin0909@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
      	"ext-gd": "*",
        "bacon/bacon-qr-code": "1.0.*",
        "illuminate/support": ">=5.0.0",
		"intervention/image": "^2.4"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6"
    },
    "autoload": {
        "psr-0": {
            "Wenxin\\Customqrcode\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wenxin\\Customqrcode\\QrCodeServiceProvider"
            ],
            "aliases": {
                "QrCodeGen": "Wenxin\\Customqrcode\\Facades\\QrCodeGen"
            }
        }
    }
}
