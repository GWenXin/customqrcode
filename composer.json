{
    "name": "wenxin/customqrcode",
    "description": "This is a customize qrcode generator.",
    "keywords": ["qrcode", "qrcodegenerator", "laravel", "customize"],
    "license" : "MIT",
    "type": "library",
    "authors": [
        {
            "name": "wenxin",
            "email": "gwenxin0909@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
      	"ext-gd": "^7.1",
        "bacon/bacon-qr-code": "^2.0",
	"intervention/image": "^2.4"
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "Wenxin\\Customqrcode\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wenxin\\Customqrcode\\QrcodeServiceProvider"
            ],
            "aliases": {
                "QrCodeGen": "Wenxin\\Customqrcode\\Facades\\QrCodeGen"
            }
        }
    }
}
